# 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# API Keys (可选)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key
TUSHARE_TOKEN=your_tushare_token

# 缓存配置
MEMORY_CACHE_TTL=300
MAX_CACHE_SIZE=100
TICKER_CACHE_TTL=30

# 日志配置
LOG_LEVEL=INFO

# CORS 配置 (生产环境请修改为实际域名)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# 网络配置
REQUEST_TIMEOUT=30
MAX_RETRIES=3
